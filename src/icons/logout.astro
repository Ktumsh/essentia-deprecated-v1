---
interface Props {
  class?: string;
  width?: string;
  height?: string;
}

const { class: className, width, height } = Astro.props;
---

<svg
  class={className}
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  ><path
    fill="none"
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="3"
    d="M7.023 5.5a9 9 0 1 0 9.953 0M12 2v8"
    color="currentColor"></path>
</svg>
