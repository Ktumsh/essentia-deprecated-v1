---
interface Props {
  class?: string;
  width?: string;
  height?: string;
}

const { class: className, width, height } = Astro.props;
---

<svg
  class={className}
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 20 20"
  ><path
    fill="currentColor"
    d="M10 3.5a6.5 6.5 0 1 1 0 13zM10 2a8 8 0 1 0 0 16a8 8 0 0 0 0-16"></path>
</svg>
